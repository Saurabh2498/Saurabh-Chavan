using System;
class Program
{
    static void Main()
    {
        int Start = 0;
    
        Console.WriteLine("Enter the Target Number : ");
        int TargetNumber = int.Parse(Console.ReadLine());
        while (Start <= TargetNumber)
        {
            Console.WriteLine(Start + "");
            Start = Start + 2;

        }
       
    }
}
